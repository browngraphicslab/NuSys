{
    "manifest_version": 2,

    "name": "nusys chrome",
    "description": "selection tool for browsers",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

    "browser_action": {
        "default_icon": "assets/icon.png"
    },
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://ajax.googleapis.com/",
        "nativeMessaging",
        "storage",
        "unlimitedStorage",
         "tabs",
         "<all_urls>"
    ],
    "web_accessible_resources": [
        "assets/*.png"
    ],
    "background": {
        "scripts": [ "jquery.js", "bg.js" ]
    }
}