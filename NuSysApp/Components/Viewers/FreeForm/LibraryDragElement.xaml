<UserControl
    x:Class="NuSysApp.LibraryDragElement"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid>
        <Grid.RenderTransform>
            <CompositeTransform ScaleX="3" ScaleY="3"></CompositeTransform>
        </Grid.RenderTransform>
        <Image x:Name="IconImage" Source="../../../Assets/icon_delete_color.png" MaxHeight="25" MaxWidth="25" Stretch="Uniform"/>
            
    </Grid>
</UserControl>
